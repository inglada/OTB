SET(ITK_CSWIG_CONFIGURATION_TYPES @ITK_CSWIG_CONFIGURATION_TYPES@)
IF(ITK_CSWIG_CONFIGURATION_TYPES)
  FILE(GLOB ITK_PYTHON_FILES "@EXECUTABLE_OUTPUT_PATH@/${CMAKE_INSTALL_CONFIG_NAME}/*.py")
ELSE(ITK_CSWIG_CONFIGURATION_TYPES)
  FILE(GLOB ITK_PYTHON_FILES "@EXECUTABLE_OUTPUT_PATH@/*.py")
ENDIF(ITK_CSWIG_CONFIGURATION_TYPES)

MESSAGE(STATUS "Installing generated python files.")

FILE(INSTALL
  DESTINATION "@CMAKE_INSTALL_PREFIX@@ITK_INSTALL_LIB_DIR@"
  TYPE FILE
  FILES ${ITK_PYTHON_FILES})
