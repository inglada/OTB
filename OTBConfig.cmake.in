@OTB_CONFIG_CODE@


# Option use external ITK.
SET(OTB_USE_EXTERNAL_ITK "@OTB_USE_EXTERNAL_ITK@")

# Option use external ITK.
SET(OTB_USE_EXTERNAL_FLTK "@OTB_USE_EXTERNAL_FLTK@")

